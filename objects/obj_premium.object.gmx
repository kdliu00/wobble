    if fade = 0 {
        increasing = true;
    }
    if fade = fadeTime {
        increasing = false;
    }
    if increasing = true {
        fade += 1;
    } else {
        fade -= 1;
    }
    
    image_xscale = 1 + (0.08 * (fade/fadeTime));
